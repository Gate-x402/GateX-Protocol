{
  "name": "gatex-protocol",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter './apps/*' --parallel dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "prettier --write .",
    "db:generate": "cd packages/common/prisma && pnpm install && pnpm generate",
    "db:migrate": "cd packages/common/prisma && pnpm migrate",
    "db:studio": "cd packages/common/prisma && pnpm studio",
    "db:push": "cd packages/common/prisma && pnpm push"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}

